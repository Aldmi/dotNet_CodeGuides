{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  
  
  "ApplicationOptions": {
    "MyDbOption": {
      "Connection": "appSet Connection",
      "Config": {
        "param1": "appSet Config.param"
      }
    },

    "MessageBrokerOption": {
      "Url": "appSet 192.168.1.2:5050",
      "TopicName": "appSet MainDataTopic"
    }
  }
}
